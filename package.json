{
  "name": "server",
  "version": "2.1.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "nodemon ./bin/www",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:migrate:undo": "npx sequelize-cli db:migrate:undo",
    "db:seed": "npx sequelize-cli db:seed:all",
    "db:seed:undo": "npx sequelize-cli db:seed:undo:all",
    "db:reset": "npx sequelize-cli db:migrate:undo:all && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "bwip-js": "^4.7.0",
    "cloudinary": "^2.9.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^17.2.3",
    "ejs": "^3.1.10",
    "express": "^4.22.1",
    "express-rate-limit": "^6.7.0",
    "express-validator": "^6.15.0",
    "express-winston": "^4.2.0",
    "helmet": "^7.0.0",
    "http-errors": "~1.6.3",
    "joi": "^18.0.1",
    "jsonwebtoken": "^9.0.2",
    "jimp": "^0.22.10",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "mysql2": "^3.14.4",
    "node-cron": "^4.2.1",
    "pg": "^8.16.3",
    "qrcode": "^1.5.4",
    "sequelize": "^6.37.7",
    "sequelize-cli": "^6.6.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.10"
  }
}
